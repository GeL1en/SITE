	function setCurrencyLocale () {
		var hostname = document.location.hostname;
		var pathname = document.location.pathname;

		var pathname_parts = pathname.split( '/' );
		var hostname_locale_pathname = hostname;

		if ( pathname_parts.length > 1 ) {
			switch ( pathname_parts[1] ) {
				case 'br':
				case 'de':
				case 'el':
				case 'fr':
				case 'it':
				case 'nl':
				case 'he':
				case 'ja':
				case 'ru':
				case 'sv':
				case 'tr':
					hostname_locale_pathname += '/' + pathname_parts[1];
					break;
			}
		}

		var currency = getQueryVar( 'currency' ) || '';
		currency     = currency || lpGetCookie( 'landingpage_currency' ) || '';

		if ( currency ) {
			switch ( currency.toLowerCase() ) {
				case 'australian':
					currency = 'AUD';
					break;
				case 'canadian':
					currency = 'CAD';
					break;
				case 'euro':
					currency = 'EUR';
					break;
				case 'pound':
					currency = 'GBP';
					break;
				case 'yen':
					currency = 'JPY';
					break;
				case 'usd':
					currency = 'USD';
					break;
				default:
					currency = currency.toUpperCase();
					break;
			}
			currency = currency || '';
		}

		if ( ! currency ) {
			currency = currency || '';
		}

		/*
		* Gets the price related values from the API response.
		*/
		function getPrices( allPrices, slug ) {
			const yearlyPrice = allPrices.prices[ 'price-' + slug + '-plan' ];
			const monthlyPrice =
				allPrices.monthly_prices[ 'price-' + slug + '-plan-monthly' ];
			const monthlyPricePerYear = monthlyPrice * 12;

			const monthlyDiscount =
				Math.floor(
					( ( monthlyPricePerYear - yearlyPrice ) / monthlyPricePerYear ) *
						100
				) || 0;

			// A part of the Introductory Offer experiment. Only selected plans and
			// currencies include an intro offer. The very presence of the `intro` keys
			// in the API response indicates enrollment in the experiment
			const monthlyPriceIntroOffer = allPrices.prices[ 'price-' + slug + '-plan-intro' ];
			const hasIntroOffer = Boolean(
				typeof monthlyPriceIntroOffer === 'number' && monthlyPriceIntroOffer > 0
			);

			return {
				yearlyPrice,
				monthlyPrice,
				monthlyPriceIntroOffer,
				monthlyPricePerYear,
				monthlyDiscount,
				hasIntroOffer,
			};
		}

		/*
		* Gets the maximum discount from all the annual plans.
		*/
		function getMaxDiscount( allPrices ) {
			const slugs = [
				...document.querySelectorAll( '.lpc-plans-table-interval-type-month[data-plan-timeframe-slug]' ),
			];
			const discounts = slugs.map( ( slug ) => {
				const { monthlyDiscount } = getPrices(
					allPrices,
					slug.getAttribute( 'data-plan-timeframe-slug' )
				);
				return monthlyDiscount;
			} );

			return Math.max( ...discounts );
		}

		/*
		* Using the discount tooltip template, replaces the %s with the maximum discount amount.
		*/
		function processDiscountTooltip( allPrices ) {
			const discountTemplates = document
				.querySelectorAll(
					'[data-discount-tooltip-template]:not([data-discount-tooltip-template=""])'
				);
			discountTemplates
				.forEach( ( discountTemplate ) => {
					const intervalToggles = discountTemplate.querySelectorAll(
						'.lpc-toggle-wrapper'
					);
					intervalToggles.forEach( intervalToggle => {
						const discountTooltip = document.createElement( 'div' );
						discountTooltip.className = 'lpc-toggle-wrapper__discount-tooltip';

						const maxDiscount = getMaxDiscount( allPrices );
						const discountTooltipTemplate = `<span>${ discountTemplate.getAttribute(
							'data-discount-tooltip-template'
						) }</span>`;

						discountTooltip.innerHTML = discountTooltipTemplate.replace(
							'%s',
							maxDiscount
						);
						intervalToggle.appendChild( discountTooltip );
					} );
				} );
		}


		changeCurrency( currency ).then( ( currency ) => {
			const allPrices = currencies[currency];

			document.querySelectorAll( '.lpc-plans-table-interval-type-annual[data-plan-timeframe]:not([data-plan-timeframe=""])' )
				.forEach( ( timeframe ) => {
					const slug = timeframe.getAttribute( 'data-plan-timeframe-slug' );

					const { yearlyPrice, hasIntroOffer } = getPrices(
						allPrices,
						slug
					);

					const timeframeText = timeframe.getAttribute(
						hasIntroOffer
							? 'data-plan-timeframe-intro'
							: 'data-plan-timeframe'
					);

					if ( ! timeframeText ) {
						return;
					}

					const price = getFormattedPrice( yearlyPrice, currency, currencies[ currency ].symbol );

					timeframe.innerHTML = timeframeText.replace( '%s', price );

					// The introductory offer pricing has a label that prefixes its
					// price value. But in order to maintain the layout of the whole
					// grid, the label has to be injected as the last element of the
					// plan subtitle container. With the following workaround, the grid
					// doesnt have to get restructed for this experiment
					if ( ! hasIntroOffer ) {
						return;
					}

					const label = timeframe.getAttribute(
						'data-plan-timeframe-intro-label'
					);

					let container = timeframe.closest( '.lpc-plans__grid' );
					if ( container ) container = container.querySelector(
						`.lpc-plan-header-${ slug } .lpc-plan-header-subtitle:not(.js-has-offer)`
					);

					if ( ! ( label && container ) ) {
						return;
					}

					const labelElement = document.createElement( 'span' );
					labelElement.className = 'lpc-plan-header-subtitle-offer lpc-plans-table-interval-type-annual';
					labelElement.innerText = label;

					container.classList.add( 'js-has-offer' );
					container.appendChild( labelElement );
				} );
			document.querySelectorAll( '.lpc-plans-table-interval-type-month[data-plan-timeframe]:not([data-plan-timeframe=""])' )
				.forEach( ( timeframe ) => {
					const timeframeText = timeframe.getAttribute(
						'data-plan-timeframe'
					);
					const slug = timeframe.getAttribute( 'data-plan-timeframe-slug' );

					const { monthlyDiscount } = getPrices( allPrices, slug );

					timeframe.innerHTML = timeframeText.replace(
						'%s',
						monthlyDiscount
					);
				} );
			processDiscountTooltip( allPrices );
		} );
	}

