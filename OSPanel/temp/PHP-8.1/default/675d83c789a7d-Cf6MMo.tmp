<script type="text/javascript">
jQuery( document ).ready( function () {
        var lazyImages = [].slice.call( document.querySelectorAll( '.shape--image div.lazy' ) );

        if ( 'IntersectionObserver' in window &&
            'IntersectionObserverEntry' in window &&
            'intersectionRatio' in window.IntersectionObserverEntry.prototype &&
            'isIntersecting' in window.IntersectionObserverEntry.prototype ) {
            var lazyImageObserver = new IntersectionObserver( function ( entries, observer ) {
                entries.forEach( function ( entry ) {
                    if ( entry.isIntersecting ) {
                        var lazyImage = entry.target;
                        if ( lazyImage.dataset.imageUrl ) {
                            lazyImage.style.backgroundImage = "url('" + lazyImage.dataset.imageUrl + "')";
                        }
                        if ( lazyImage.dataset.imageUrlRetina ) {
                          var mq = false;
                          if ( window.matchMedia ) {
                            mq = window.matchMedia("only screen and (min--moz-device-pixel-ratio: 1.5), only screen and (-o-min-device-pixel-ratio: 3/2), only screen and (-webkit-min-device-pixel-ratio: 1.5), only screen and (min-device-pixel-ratio: 1.5), only screen and (min-resolution: 1.5dppx)");
                          }
                          if ( mq && mq.matches || (window.devicePixelRatio > 1) ) {
                            lazyImage.style.backgroundImage = "url('" + lazyImage.dataset.imageUrlRetina + "')";
                          }
                        }
                        lazyImage.classList.remove( 'lazy' );
                        lazyImage.classList.add( 'lazy_loaded' );
                        lazyImageObserver.unobserve( lazyImage );
                    }
                } );
            } );

            lazyImages.forEach( function ( lazyImage ) {
                lazyImageObserver.observe( lazyImage );
            } );
        } else {
            lazyImages.forEach( function ( lazyImage ) {
                if ( lazyImage.dataset.imageUrl ) {
                    lazyImage.style.backgroundImage = "url('" + lazyImage.dataset.imageUrl + "')";
                }

                lazyImage.classList.remove( 'lazy' );
                lazyImage.classList.add( 'lazy_loaded' );
            } );
        }
    }
);
