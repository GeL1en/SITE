	// track when plans are in view
	( function() {
		var plansSeen = false;
		var updating = false;
		var plansTable = document.querySelector( '.plan.card' );
		if ( ! plansTable ) {
			return;
		}
		var checkPlansNear = function() {
			updating = true;
			if ( ! plansSeen ) {
				if ( plansTable.getBoundingClientRect().top < 600 ) {
					plansSeen = true;
					window._tkq = window._tkq || [];
					window._tkq.push( [ 'recordEvent', 'calypso_wp_plans_test_view' ] );
				}
			}
			updating = false;
		};
		// Fire this on page load and scroll.
		// Some pages might show the plans at the top and will be visible without any scrolling
		window.addEventListener( 'load', function( ) {
			updating = updating || window.requestAnimationFrame( checkPlansNear );
		} );
		window.addEventListener( 'scroll', function( ) {
			updating = updating || window.requestAnimationFrame( checkPlansNear );
		} );
	} )();

