	/**
	* Change currency
	*/
	async function changeCurrency( iso ) {
		if ( ! iso || ! currencies.hasOwnProperty( iso ) ) {
			try {
				const data = await maybeProxyFetch( {
					path: '/landing-page/prices',
					apiNamespace: 'rest/v1.3',
					query: {
						http_envelope: 1,
					}
				} )
				iso = data.currency;
			} catch ( e ) {
				console.error( e );
			}
		}

		var intervalTypeProps = [
			{
				intervalTypeKey: 'prices',
				formatterFunction: getFormattedMonthlyPrice,
				timeframeOff: window.homepage ? window.homepage.timeframeOff : 0,
				class: 'lpc-plans-table-interval-type-annual'
			},
			{
				intervalTypeKey: 'monthly_prices',
				formatterFunction: getFormattedPrice,
				timeframeOff: window.homepage ? window.homepage.timeframeOffMonthly: 0,
				class: 'lpc-plans-table-interval-type-month'
			}
		];

		intervalTypeProps.forEach( function( intervalTypeProp ) {
			var pricesForCurrency = currencies[iso][ intervalTypeProp.intervalTypeKey ];
			for ( var i in pricesForCurrency ) {
				var plan_value_element = jQuery ( '.' + i );

				if ( ! plan_value_element.length ) {
					continue;
				}

				var origPrice = pricesForCurrency[ 'orig-' + i ] || 0;
				var rawPrice = pricesForCurrency[i];

				// A part of the Introductory Offer experiment. Only selected plans and
				// currencies include an intro offer. The very presence of the `intro` keys
				// in the API response indicates enrollment in the experiment
				const introOfferPrice = pricesForCurrency[ `${ i }-intro` ];
				if ( typeof introOfferPrice === 'number' && introOfferPrice > 0 ) {
					rawPrice = introOfferPrice;
				}

				var adjustedRawPrice = intervalTypeProp.intervalTypeKey === 'monthly_prices' ? rawPrice : rawPrice / 12;
				if ( adjustedRawPrice > 99000 ) {
					plan_value_element.parent().addClass( 'is-large-currency' );
				}
				// Needed for Plans comparisong grid where space is more limited.
				if ( adjustedRawPrice > 9000 ) {
					plan_value_element.parent().addClass( 'is-medium-currency' );
				}
				var fullPrice = intervalTypeProp.formatterFunction( rawPrice, iso, currencies[iso].symbol );
				var origFullPrice = intervalTypeProp.formatterFunction( origPrice, iso, currencies[iso].symbol );

				var decodedSymbol = htmlDecode( currencies[iso].symbol ).replace('$', '\\$');
				var symbolPattern = /(^[^\d]*)/;
				var	origPriceMarkup = origPrice ? '<del>' + origFullPrice.replace( symbolPattern, '<sup>$1</sup>' ) + '</del> ': '';

				plan_value_element.html( origPriceMarkup + '<span>' + fullPrice.replace( symbolPattern, '<sup>$1</sup>' ) + '</span>' );
			}
		});

		currencyISO = iso; // Change currency.
		lpSetCookie( 'landingpage_currency', iso, 365 );

		return iso;
	}

	/**
